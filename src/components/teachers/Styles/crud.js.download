            bootbox.confirm(settings.confirmMessages[invoke], function(result) {
                if (result){
                    var object = {
                        id: item_id,
                        invoke: invoke,
                        context : object_id,
                        addition : addition,
                        item_addition: item_addition
                    };
                        
                    if(addition_name != '') {
                        eval('object.'+addition_name+' = '+addition+';');
                    }
                    
                    manageSelectedAJAX(object);
               }
            }); 
